#include <iostream>

int main() {
    int number;
    std::cout << "Введіть ціле число: ";
    std::cin >> number;

    std::cout << "Числа, на які " << number << " ділиться без залишку: " << std::endl;

    for (int i = 1; i <= abs(number); ++i) {
        if (number % i == 0) {
            std::cout << i << std::endl;
        }
    }

    return 0;
}
