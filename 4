#include <iostream>

int main() {
    int A;
    std::cout << "Введіть довільне ціле число A: ";
    std::cin >> A;

    std::cout << "Цілі числа B, для яких A ділиться без залишку на B*B і не ділиться без залишку на B*B*B:" << std::endl;

    for (int B = 1; B * B <= A; ++B) {
        if (A % (B * B) == 0 && A % (B * B * B) != 0) {
            std::cout << B << std::endl;
        }
    }

    return 0;
}
